const NodeGeocoder = require('node-geocoder');
async function get () { 
    const options = {
	provider: 'google',
	apiKey: 'AIzaSyAeCUbyvfoZZkrjS11B0kp9WNVtqb_fFWs', // for Mapquest, OpenCage, Google Premier
	formatter: null // 'gpx', 'string', ...
    };
 
    const geocoder = NodeGeocoder(options);
 
    // Using callback
    const res = await geocoder.geocode('29 champs elys√©e paris');
    console.log(res[0])
}
get()
